<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   
   

	<bean id="question"
		class="com.nishu.bank.model.Question">      
		
		 <property name = "questionId" value = "1432"/>
		 <property name = "question" value = " Place "/>
		
      <property name = "answerList">
         <list>
            <value> Name</value>
            <value> Birthday</value>
            <value>Varanasi</value>
            <value>Varanasi</value>
         </list>
      </property>

      <property name = "answerSet">
         <set>
		 	<value> Name</value>
            <value> Birthday</value>
            <value>Varanasi</value>
            <value>Varanasi</value>
         </set>
      </property>

      <property name = "answerMap">
         <map>
            <entry key = "1" value = "Name"/>
            <entry key = "2" value = "Birthday"/>
            <entry key = "3" value = "Varanasi"/>
            <entry key = "4" value = "Varanasi"/>
         </map>
      </property>
</bean>

</beans>